{
  "name": "aem-mle-sync",
  "version": "1.0.0",
  "description": "AEM Assets to MLE (Media Logic Engine) metadata synchronization service",
  "type": "module",
  "main": "aem-mle-sync-service.js",
  "scripts": {
    "start": "node aem-mle-sync-service.js",
    "dev": "nodemon aem-mle-sync-service.js",
    "test": "jest",
